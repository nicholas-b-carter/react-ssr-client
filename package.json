{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev:build-server": "webpack --config webpack.server.js --watch --mode development",
    "dev:build-client": "webpack --config webpack.client.js --watch --mode development",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev": "npm-run-all --parallel dev:*",
    "build:server": "webpack --config webpack.server.js --mode production",
    "build:client": "webpack --config webpack.client.js --mode production",
    "build": "NODE_ENV=production npm-run-all --parallel build:*",
    "serve": "node build/bundle.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.2.2",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "^6.26.0",
    "compression": "1.7.0",
    "concurrently": "3.5.0",
    "express": "4.15.4",
    "express-http-proxy": "1.0.6",
    "file-loader": "^1.1.7",
    "lodash": "4.17.4",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.4.0",
    "styled-components": "^3.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "ignore-styles": "^5.0.1",
    "isomorphic-style-loader": "^4.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.6",
    "null-loader": "^0.1.1",
    "postcss-less": "^1.1.3",
    "postcss-loader": "^2.1.1",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.2"
  }
